{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.js",
    "**/*.jsx",
    "lib/data.js",
    "lib/practive.js",
    "pages/_document.tsx",
    "pages/_app.tsx",
    "components/Knowledge.tsx",
    "components/Banner.tsx",
    "pages/resume.tsx",
    "pages/work.tsx",
    "components/Addr",
    "components/BgBorder.tsx",
    "components/FollowMe.tsx",
    "components/home/Introduce.tsx",
    "components/Service.tsx",
    "components/Projects.jsx",
    "components/MainTag.tsx",
    "components/Objective.tsx",
    "components/Navber.jsx",
    "components/MyDesign.tsx",
    "components/LanguageSkills.tsx",
    "components/Layout.jsx",
    "ess.tsx",
    "pages/index.tsx",
    "pages/api/getData.ts"
  ],
  "exclude": ["node_modules"]
}
